@using pracAsp.Models
@model Customer
@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<h2>Details of Customer</h2>

<div class="row">
    <div class="col-md-3">
        <ul class="list-group">
            <li class="list-group-item"><b>Product Id</b>@Model.Id</li>
            <li class="list-group-item"><b>Product Name</b>@Model.Name</li>
            <li class="list-group-item"><b>Price :</b>@Model.MobileNo</li>
            
        </ul>
    </div>
</div>
<div class="row">
    <div class="col-md-3">
        @Html.ActionLink("Back to Index ", "Index", "Products")
    </div>
</div>